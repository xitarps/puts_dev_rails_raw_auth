<h2> Sign in</h2>

<%= form_with(model: @user, url: signin_path) do |f| %>
  <div class='field'>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

   <div class='field'>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div class='actions'>
    <%= f.submit 'Sign in' %>
  </div>
<% end %>
